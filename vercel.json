{
  "name": "proyectovideo",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@proyectovideo-db-user",
    "DB_PASSWORD": "@proyectovideo-password",
    "DB_HOST": "@proyectovideo-db-host",
    "DB_NAME": "@proyectovideo-db-name"
  }
}
